{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
  ],
  "rewrites": [
    { "source": "/api/todos/(.*)", "destination": "https://todo-backend-euoz.onrender.com/api/todos/$1" }
  
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}